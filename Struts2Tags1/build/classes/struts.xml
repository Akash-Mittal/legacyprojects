<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd ">
<struts>
	<package name="mydefault" extends="struts-default">
	
	<action name="cityAction" class="pkg1.actions.CityAction">
		<result>/yourcity.jsp</result>
	</action>
	
	
	
	<action name="iterateAction" class="pkg1.actions.IterateAction">
		<result>/iterator.jsp</result>
	</action>
	<action name="personAction" class="pkg1.actions.PersonAction">
		<result>/user.jsp</result>
	</action>
		<action name="personalAction" class="pkg1.actions.PersonalAction">
		<result name="input">personal_info.jsp</result>
		<result name="success">info1.jsp</result>
	</action>	
	
	<action name="generalAction" class="pkg1.actions.GeneralAction">
		<result name="input">general_info.jsp</result>
		<result name="success">info2.jsp</result>
	</action>
	
	
	
	</package>
</struts>

