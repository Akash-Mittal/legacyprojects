<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config />
	<bean id="add" class="pkg11.resource.Address">
		<property name="street" value="Sector-57 add" />
		<property name="city" value="Noida" />
		<property name="zip" value="1234567" />
	</bean>

<bean id="add1" class="pkg11.resource.Address">
		<property name="street" value="Sector-57 add1" />
		<property name="city" value="Noida" />
		<property name="zip" value="1234567" />
	</bean>
	
	<bean id="emp" class="pkg11.resource.Employee">
		<property name="eid" value="A005" />
		<property name="name" value="Subodh Kumar" />
		<property name="salary" value="666666" />
		<property name="address"  ref="add13"/>
	</bean>

</beans>
