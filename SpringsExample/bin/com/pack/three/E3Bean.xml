<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean name="add" class="com.pack.three.Address">
<property name="city"><value>123</value></property>
<property name="pincode"><value>201001</value></property>
</bean>

<bean name="emp3" class="com.pack.three.Employee">
<property name="address" ref="add"></property>
<property name="empid"> <value>79480</value></property>
<property name="name"><value>Akash mittal</value></property>
</bean>
</beans>
